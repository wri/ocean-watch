{
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "prettier/prettier": [
      "error",
      {},
      {
        "usePrettierrc": true
      }
    ],
    "no-console": [1, { "allow": ["info"] }],
    "import/no-anonymous-default-export": [2, { "allowObject": true }]
  }
}
